{
	"name": "templating",
	"version": "1.2.1",
	"description": "eXist-db HTML Templating Library",
	"homepage": "https://github.com/eXist-db/templating#readme",
	"bugs": "https://github.com/eXist-db/templating/issues",
	"keywords": [
		"exist",
		"exist-db",
		"xml",
		"xql",
		"xquery"
	],
	"devDependencies": {
		"@existdb/gulp-exist": "^4.3.2",
		"@existdb/gulp-replace-tmpl": "^1.0.4",
		"axios": "^0.21.1",
		"chai": "^4.2.0",
		"chai-xml": "^0.4.0",
		"chokidar": "^3.5.3",
		"delete": "^1.1.0",
		"fs-extra": "^9.1.0",
		"glob-stream": "^7.0.0",
		"gulp": "^4.0.2",
		"gulp-rename": "^2.0.0",
		"gulp-zip": "^5.0.2",
		"jsdom": "^16.4.0",
		"mocha": "^10.1.0",
		"xmldoc": "^1.1.2"
	},
	"author": {
		"name": "The eXist-db Authors"
	},
	"license": "LGPL-2.1",
	"scripts": {
		"start": "npm install && npm run build",
		"test": "gulp install:all && npm run test:mocha && npm run test:xqsuite",
		"test:mocha": "mocha test/mocha --recursive --exit",
		"test:xqsuite": "node test/xqs/xqSuite.js",
		"test:watch": "mocha test/mocha --recursive --watch",
		"build": "gulp build",
		"build:all": "gulp build:all"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/eXist-db/templating",
		"license": "LGPL-2.1"
	},
	"funding": {
		"type": "opencollective",
		"url": "https://opencollective.com/existdb"
	},
	"overrides": {
		"chokidar": "$chokidar",
		"glob-stream": "$glob-stream"
	}
}
